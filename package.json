{
  "name": "chat-bot-saas",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "npm run generate-blog-cache && next build && node scripts/post-build.js",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0 || true",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "postbuild": "node scripts/post-build.js",
    "generate-blog-cache": "node scripts/generate-blog-cache.js",
    "generate-blog-images": "node scripts/generate-blog-images.js",
    "db:push:test": "SUPABASE_ACCESS_TOKEN=$TEST_SUPABASE_ACCESS_TOKEN supabase db push --project-ref $TEST_SUPABASE_PROJECT_REF",
    "test:rls": "vitest run tests/rls.integration.test.ts",
    "check:rls": "npm run db:push:test && npm run test:rls"
  },
  "dependencies": {
    "@dqbd/tiktoken": "^1.0.21",
